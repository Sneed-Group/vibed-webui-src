# Server Configuration
# This is the URL of your Ollama API server.
# IMPORTANT: 
# - Do not include a trailing slash
# - For local Ollama instances, typically use http://localhost:11434
# - If your Ollama API is behind a path (e.g., /api), include that path
VITE_OLLAMA_API_URL=http://localhost:11434

# UI Configuration
# Port where the web UI server will run
VITE_UI_PORT=8765

# Production Mode
# Set to TRUE to hide the server connection URL in the footer
VITE_PROD=TRUE 